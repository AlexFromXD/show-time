{
	"name": "showtime",
	"displayName": "ShowTime",
	"description": "convert unix timestamp to readable datetime",
	"version": "0.0.3",
	"icon": "time.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/AlexFromXD/show-time.git"
	},
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"timestamp",
		"date",
		"convert",
		"time"
	],
	"activationEvents": [
		"onCommand:extension.showTime"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.showTime",
				"title": "Show Time"
			}
		],
		"keybindings": [
			{
				"command": "extension.showTime",
				"key": "alt+d",
				"mac": "cmd+d",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"@typescript-eslint/parser": "^2.18.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	},
	"publisher": "AlexFromXD"
}
